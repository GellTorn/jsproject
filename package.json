{
  "name": "jsproject",
  "version": "0.0.1",
  "description": "game framework",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-source && npm run build-demo1",
    "build-source": "tsc && cd ./compiled/src/ && browserify Camera.js Circle.js Ellipse.js Entity.js Game.js Physics.js Rectangle.js Scene.js Sprite.js Text.js Vector2.js Spritesheet.js Animation.js -o main.js && cd ../../",
    "build-demo1": "cd ./compiled/js/ && browserify demo1.js -o demo1.js && cd ../../",
    "web": "tsc && webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GellTorn/jsproject.git"
  },
